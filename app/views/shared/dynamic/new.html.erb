<% @title = "Nouveau '#{@controller_setup[:model_name]}'" %>
<%= form_tag({}, { :name => 'create_form', :class => 'form-horizontal' }) do%>
	<div class="center-menu">
    <ul>
		<li class="btn"><%= render :partial => 'shared/goto_menu_item', :locals => {:text => "Sauver",:image => "appli/save_32.png", :action => "create",  :form => "create_form"}%></li>
		<li class="btn"><%= render :partial => 'shared/goto_menu_item', :locals => {:back => true, :text => "Retour",:image => "appli/back_32.png", :action => "list", :param_goto => {:parent_id => @back_to_parent}}%></li>
    </ul>
		<div class="clearer"></div>
	</div>
	
	<!-- Error messages -->
	<%= errors_for(@instance) %>
	
	<div>
	  <%= render :partial => 'shared/dynamic/form' %>
	</div>



<% end %>
